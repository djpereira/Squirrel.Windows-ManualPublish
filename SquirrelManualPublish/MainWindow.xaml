<Window x:Class="SquirrelManualPublish.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SquirrelManualPublish"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel VerticalAlignment="Center">
        <TextBlock Text="Squirrel Manual Publish" HorizontalAlignment="Center" />
        <Button Content="Update" HorizontalAlignment="Center" Padding="5" Margin="5" Click="Button_Click" />
        <TextBlock x:Name="Message" HorizontalAlignment="Center" />
    </StackPanel>
</Window>
